/usr/bin/foo {
  allow /{usr/,}bin/cat rm,
  allow /{usr/,}bin/cat ix,
  allow /{usr/,}bin/true px,
  allow /{usr/,}bin/true m,
  allow /{usr/,}bin/false m,
  allow /{usr/,}bin/false ux,
  allow /lib/libc.so rwl,
  allow /lib/libc.so m,
}
