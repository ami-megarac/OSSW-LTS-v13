#
#=DESCRIPTION More basic parsing test of local (internal) profiles
#=EXRESULT PASS
# vim:syntax=subdomain
# Last Modified: Sun Apr 17 19:44:44 2005
#
/does/not/exist {
  #include <includes/base>

  /usr/X11R6/lib/lib*so* rrr,
  /does/not/exist r,
  /var/log/messages www,
  /bin/ls ixixixix,
  /bin/echo uxuxuxuxux,

  profile /bin/grep {
    #include <includes/base>

    /bin/grep r,
    /tmp/shmeegol rwm,
  }

  capability setuid,

  profile /bin/cat {
    #include <includes/base>

    /bin/cat r,
    /tmp/shmeegol w,
  }

  /tmp/sd*.foo rwrwwrll,
  /bin/cat pxpxpxpxpx,
}

