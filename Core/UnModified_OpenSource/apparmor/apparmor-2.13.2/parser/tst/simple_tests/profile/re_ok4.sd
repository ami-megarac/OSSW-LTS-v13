:ns:/** {
  #include <includes/base>

  /usr/X11R6/lib/lib*so* rrr,
  /does/not/exist r,
  /var/log/messages www,
  /tmp/sd*.foo rwrwwrll,
  /bin/cat pxpxpxpxpx,
  /bin/ls ixixixix,
  /bin/echo uxuxuxuxux,
}

