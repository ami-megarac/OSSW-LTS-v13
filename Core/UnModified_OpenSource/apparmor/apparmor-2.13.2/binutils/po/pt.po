#: ../aa_enabled.c:26
#, c-format
msgid ""
"%s: [options]\n"
"  options:\n"
"  -q | --quiet        Don't print out any messages\n"
"  -h | --help         Print help\n"
msgstr ""
"%s: [opções]\n"
"  opções:\n"
"  -q | --silencioso        Não mostrar mensagens\n"
"  -h | --ajuda         Mostar ajuda\n"

#: ../aa_enabled.c:45
#, c-format
msgid "unknown or incompatible options\n"
msgstr "opções desconhecidas ou incompatíveis\n"

#: ../aa_enabled.c:55
#, c-format
msgid "unknown option '%s'\n"
msgstr "opção desconhecida '%s'\n"

#: ../aa_enabled.c:64
#, c-format
msgid "Yes\n"
msgstr "Sim\n"

#: ../aa_enabled.c:71
#, c-format
msgid "No - not available on this system.\n"
msgstr "Não - não disponível neste sistema.\n"

#: ../aa_enabled.c:74
#, c-format
msgid "No - disabled at boot.\n"
msgstr "Não - desligado ao iniciar.\n"

#: ../aa_enabled.c:77
#, c-format
msgid "Maybe - policy interface not available.\n"
msgstr "Talvez - política de interface não disponível.\n"

#: ../aa_enabled.c:81
#, c-format
msgid "Maybe - insufficient permissions to determine availability.\n"
msgstr "Talvez - permissões insuficientes para determinar disponibilidade.\n"

#: ../aa_enabled.c:84
#, c-format
msgid "Error - '%s'\n"
msgstr "Erro - '%s'\n"
